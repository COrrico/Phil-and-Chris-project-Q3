	<body>
				<button onclick="start()"> Presh to input a car </button>
		<script>
				var type = prompt("Is this a motorcycle or a car")
				var inventory =[];
				
			
				
								
			 //Add cars to Array
			 
				funciton start()
					{
					if( type == "car")   
						{
						
						function car(mileage, color) = {          
							this.mileage;  
							this.color;      
							}
						var mileage1 = prompt("What is the mileage of the car?")
						var color1 = prompt("What is the color of the car?")
						
						var car = new car(mileage1,color1)	
						
						var inventory += car;		
						}
						
					else if(type == "motorcycle")
						{
						   function motocycle(mileage, color) = {
							this.mileage;  
							this.color; 
							}   	
						var mileage1 = prompt("What is the mileage of the car?") 
						var color1 = prompt("What is the color of the car?")
						
						var motocycle = new motocycle(mileage1,color1)	
						
						var inventory += motocycle;
						
						}
					}
			//search for cars.
			
		</script>   
		
